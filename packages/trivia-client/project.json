{
  "name": "trivia-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "trivia-client:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "trivia-client:build:development"
        }
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/trivia-client"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    }
  },
  "tags": []
}
